time.points<-seq(0,15,0.1)
ip.covid19<-dlnorm(time.points,meanlog = log(5.2),sdlog = log(1.7))
ip.fluA<-dlnorm(time.points,meanlog = log(1.4),sdlog = log(1.51))
#ip.rsv<-dlnorm(time.points,meanlog = log(4.4),sdlog = log(1.24))
inf.covid19<-dgamma(time.points,shape = 12, rate = 2.08)/ (pgamma(15,shape = 2,rate = 2.08))
inf.fluA<-dgamma(time.points,shape = 3.5, rate = 1.15)/ (pgamma(6.24,shape = 3.5,rate = 1.15))
Inc.Periods<-data.frame(x=rep(time.points,3),y=c(ip.covid19,ip.fluA,ip.rsv), Disease=c(rep("COVID-19",length(time.points)),rep("FLU-A",length(time.points)),rep("RSV",length(time.points))), Quantity=rep("IncubationPeriod",length(time.points)*3) )
Inc.Periods<-data.frame(x=rep(time.points,2),y=c(ip.covid19,ip.fluA), Disease=c(rep("COVID-19",length(time.points)),rep("FLU-A",length(time.points))), Quantity=rep("IncubationPeriod",length(time.points)*2) )
Inf.Measure<-data.frame(x=rep(time.points,2),y=c(inf.covid19,inf.fluA) , Disease=c(rep("COVID-19",length(time.points)),rep("FLU-A",length(time.points))), Quantity=rep("InfectiousnessMeasure",length(time.points)*2))
dt<-data.frame(x=c(Inc.Periods$x,Inf.Measure$x), y=c(Inc.Periods$y,Inf.Measure$y),Disease=c(as.character(Inc.Periods$Disease),as.character(Inf.Measure$Disease)), Quantity=c(as.character(Inc.Periods$Quantity),as.character(Inf.Measure$Quantity)))
pt<-data.frame(x=c(0,0), y=c(0,0), Quantity=c("IncubationPeriod","InfectiousnessMeasure"))
ggplot(dt, aes(x=x, y=y))+geom_line(data = Inc.Periods, aes(x=x,y=y, col=Disease),lwd=1.3,lty=1)+geom_line(data = Inf.Measure, aes(x=x, y=y, col=Disease),lwd=1.3,lty=2)+
theme_minimal()+theme(
legend.key.size = unit(0.8, "cm"),
legend.key.width = unit(0.8,"cm"),
legend.title = element_text(size=17, face = "bold"),
legend.text = element_text(size=16),
axis.text.x = element_text(size=16),
axis.title = element_text(size=20),
axis.text.y = element_text(size=16),
text = element_text(size=20, face = "bold"),
) + xlab("Days")+ylab("Probability Density")+scale_color_brewer(palette = "Dark2")
